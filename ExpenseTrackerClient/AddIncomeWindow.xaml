<Window x:Class="ExpenseTrackerClient.AddIncomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExpenseTrackerClient"
        mc:Ignorable="d"
        Title="AddIncomeWindow" Height="200" Width="350">
    <Window.Background>
        <ImageBrush ImageSource="pack://application:,,,/Images/AddIncome.png"/>
    </Window.Background>
    <Grid RenderTransformOrigin="0.491,0.296">
        <Button Content="" Background="Transparent" BorderBrush="Transparent" Click="Button_Click_Registration" HorizontalAlignment="Left" Margin="156,144,0,0" VerticalAlignment="Top" Width="30" Height="26"/>
        <ComboBox x:Name="CategoryIncome" Margin="114,13,121,153">
            <ComboBoxItem Content="Зарплата"/>
            <ComboBoxItem Content="Кредит"/>
            <ComboBoxItem Content="Подарок"/>
        </ComboBox>
        <TextBox  Background="Transparent" BorderBrush="Transparent" HorizontalAlignment="Left" Margin="114,46,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="114"/>

    </Grid>
</Window>
