<Window x:Class="ExpenseTrackerClient.SearchResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ExpenseTrackerClient"
        mc:Ignorable="d"
        Title="SearchResultsWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Width="400" Height="30" Margin="10" PlaceholderText="Введите текст для поиска..."/>
            <ListBox Name="ResultsListBox" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>